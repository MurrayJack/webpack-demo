{
  "compilerOptions": {
      "outDir": "./lib/",
      "sourceMap": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "moduleResolution": "node",
      "declaration": true,
      "lib": [
          "es2015",
          "dom"
      ]
  },
  "include": [
      "./src/**/*",
  ]
}